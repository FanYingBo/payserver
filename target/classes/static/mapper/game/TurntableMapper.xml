<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.last.pay.core.db.mapper.gamedb.TurntableMapper">
 	<select id="getAllTurnTables" resultType="com.last.pay.core.db.pojo.Turntable" >
		SELECT 
			Position position,
			Type type,
			Amount amount,
			PropId propId,
			Radio radio
		from 
			Turntable
	</select>
 </mapper>