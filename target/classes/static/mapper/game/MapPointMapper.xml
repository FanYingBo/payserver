<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.last.pay.core.db.mapper.gamedb.MapPointMapper">
 	<select id="getAllMapPoints" resultType="com.last.pay.core.db.pojo.MapPoint" >
		SELECT 
			Position position,
			Type type,
			Toll toll,
			Content content,
			DynamicId dynamicId
		from 
			MapPoint
	</select>
 </mapper>